{
  "name": "redux-xforms",
  "version": "1.0.0",
  "description": "Composable reducer transformations for redux",
  "main": "lib/index.js",
  "file": [
    "lib",
    "src"
  ],
  "scripts": {
    "docs": "esdoc",
    "build": "babel src -d lib --ignore \"**/*.spec.js\"",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf lib",
    "lint": "eslint --color --ext .js src test examples build",
    "lint:watch": "esw --color --watch --ext .js app test examples build",
    "postbuild": "npm test",
    "posttest": "npm run lint",
    "prepublish": "in-publish && npm run clean && npm run build || not-in-publish",
    "test": "jest --config jest.config.json",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhelmer/redux-xforms.git"
  },
  "keywords": [
    "redux",
    "reducer",
    "transformations",
    "transducers"
  ],
  "author": "Magnus Helmersson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mhelmer/redux-xforms/issues"
  },
  "homepage": "https://github.com/mhelmer/redux-xforms#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "esdoc": "^0.5.2",
    "eslint": "^3.18.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.0.1",
    "in-publish": "^2.0.0",
    "jest": "^19.0.2",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "esdoc-importpath-plugin": "^0.1.1",
    "redux": "^3.6.0"
  }
}
