{
  "source": "./src",
  "destination": "./doc",
  "experimentalProposal": {
    "objectRestSpread": true
  },
  "test": {
    "type": "mocha",
    "source": "./src",
    "includes": [".*\\.spec\\.(js|es6)$"]
  },
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [ {
          "from": "^src/",
          "to": "lib/"
        } ]
      }
    }
  ]
}
